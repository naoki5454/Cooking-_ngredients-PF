<h3 class="text-center mt-5">料理の一覧</h3>
  <div class="side_side ml-5 pl-5">
    <% @cuisines.each do |cuisine| %>
    <div>
      <%= attachment_image_tag cuisine, :cuisine_image, :fill, 100, 100, format: 'jpeg' %>
      <div class="mt-3">
        <%= link_to admins_cuisine_path(cuisine) do %>
        <%= cuisine.cuisine_name %>
        <% end %>
      </div>
      <h4 class="mt-3">調理時間</h4>
      <%= cuisine.time %>分
    </div>
    <% end %>
  </div>
<%= paginate @cuisines %>
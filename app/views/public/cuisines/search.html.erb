<div class="container">
  <div class="row col-10 mx-auto">
    <table class="table table-hover table-inverse">
      <thead>
        <tr>
          <th class="kensaku">検索結果</th>
        </tr>
      </thead>
    <% if @range == '1' %>
      <tbody>
        <% @cuisine.each do |cuisine| %>
          <tr>
            <td class="text-right">
              <%= link_to cuisine_path(cuisine) do %>
              <%= cuisine.cuisine_name %>
              <% end %>
            </td>
          </tr>
        <% end %>
      </tbody>
      <% else @range == '2' %>
      <tbody>
        <% @genres.each do |genre| %>
          <tr>
            <td class="text-center">
              <%= link_to genre_path(genre) do %>
              <%= genre.genre_name %>
              <% end %>
            </td>
          </tr>
          <% end %>
      </tbody>
    <% end %>
    </table>
  </div>
</div>
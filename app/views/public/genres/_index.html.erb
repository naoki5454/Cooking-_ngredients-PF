<table class="ml-5 mt-3">
  <tr>
    <td>ジャンル名</td>
  </tr>
  <% genres.each do |genre| %>
  <tr>
    <td height="50px">
      <%= link_to genre_path(genre) do %>
      <%= genre.genre_name %>
      <% end %>
    </td>
  </tr>
  <% end %>
  <tr>
    <td><%= link_to "もっと見る", genres_path, class: "color2" %></td>
  </tr>
</table>